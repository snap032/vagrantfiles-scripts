Vagrant.configure("2") do |config|
  
  config.vm.define "client3" do |client3|
    client3.vm.box = "ubuntu/xenial64"
    client3.vm.hostname = "client3"
  end



  config.vm.define "client5" do |client5|
    client5.vm.hostname = "client5"
    client5.vm.box = "centos/7"
  end


  
  config.vm.define "client4" do |client4|
    client4.vm.box = "ubuntu/xenial64"
    client4.vm.hostname = "client4"
  end



  config.vm.define "client6" do |client6|
    client6.vm.hostname = "client6"
    client6.vm.box = "centos/7"
  end
end


